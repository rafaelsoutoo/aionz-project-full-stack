generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Produto {
  id        String   @id @default(cuid())
  nome      String
  descricao String
  preco     Float
  categoria String
  imagem    String
  createdAt DateTime @default(now())

  @@map("produtos")
}
